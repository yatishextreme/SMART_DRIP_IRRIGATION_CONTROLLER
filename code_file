        (dt,vd1nocycles,vd1cycletypes,vd2nocycles,vd2cycletypes,vd3nocycles,vd3cycletypes)=totaltime(x1)
        totalirrentry.configure(text=dt)
        totalsec=getSec(dt)
        vd1sec=getSec1(x1[5])
        vd2sec=getSec1(x1[6])
        vd3sec=getSec1(x1[7])
