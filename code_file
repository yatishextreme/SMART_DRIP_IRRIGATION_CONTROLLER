def getSec1(s): #hh:mm to seconds
    l = s.split(':')
    return int(l[0]) * 3600 + int(l[1]) * 60 



def getSec(s):    #hh:mm:ss to seconds
    l = s.split(':')
    return int(l[0]) * 3600 + int(l[1]) * 60 + int(l[2])
