def savebuttonpressed():
   print("saving all the settings")
   myfiletypes = [('setting files', '*.txt')]
   filename=asksaveasfilename(filetypes = myfiletypes,initialdir='/home/yatish/Desktop/spe_workspace/settings',title='Save the program')
   settinglist_pump=[checkpump1.get(),pump1rate.get(),checkpump2.get(),pump2rate.get(),checkpump3.get(),
                        pump3rate.get(),checkpump4.get(),pump4rate.get(),checkpump5.get(),pump5rate.get(),checkpump6.get(),
                        pump6rate.get()]
   print(settinglist_pump)
   settinglist_venturi=[checkventuri1.get(),venturi1rate.get(),checkventuri2.get(),venturi2rate.get(),
                        checkventuri3.get(),venturi3rate.get(),checkventuri4.get(),venturi4rate.get(),
                        checkventuri5.get(),venturi5rate.get(),checkventuri6.get(),venturi6rate.get(),
                        checkventuri7.get(),venturi7rate.get(),checkventuri8.get(),venturi8rate.get(),
                        checkventuri9.get(),venturi9rate.get()]
   settinglist_valve=[checkvalve1.get(),valve1rate.get(),checkvalve2.get(),valve2rate.get(),checkvalve3.get(),valve3rate.get(),
                        checkvalve4.get(),valve4rate.get(),checkvalve5.get(),valve5rate.get(),checkvalve6.get(),valve6rate.get(),
                        checkvalve7.get(),valve7rate.get(),checkvalve8.get(),valve8rate.get(),checkvalve9.get(),valve9rate.get(),
                        checkvalve10.get(),valve10rate.get(),checkvalve11.get(),valve11rate.get(),checkvalve12.get(),valve12rate.get(),
                        checkvalve13.get(),valve13rate.get(),checkvalve14.get(),valve14rate.get(),checkvalve15.get(),valve15rate.get(),
                        checkvalve16.get(),valve16rate.get(),checkvalve17.get(),valve17rate.get(),checkvalve18.get(),valve18rate.get(),
                        checkvalve19.get(),valve19rate.get(),checkvalve20.get(),valve20rate.get(),checkvalve21.get(),valve21rate.get(),
                        checkvalve22.get(),valve22rate.get(),checkvalve23.get(),valve23rate.get(),checkvalve24.get(),valve24rate.get(),
                        checkvalve25.get(),valve25rate.get(),checkvalve26.get(),valve26rate.get(),checkvalve27.get(),valve27rate.get(),
                        checkvalve28.get(),valve28rate.get(),checkvalve29.get(),valve29rate.get(),checkvalve30.get(),valve30rate.get()]
   print(settinglist_venturi)
   print(settinglist_valve)
   
   outfile=open(filename,'wb')
   pickle.dump(settinglist_pump,outfile)
   pickle.dump(settinglist_venturi,outfile)
   pickle.dump(settinglist_valve,outfile)



def exitbuttonpressed():
    quit()
    print("do nothing")

    



def settingpagefun():
    root1=Tk()
    root1.geometry('1020x600')
    root1.title("SETTING PAGE")
    root1.configure(background = '#a6a6a6')
    root1.resizable(FALSE,FALSE)
    savebutton=Button(root1,text="SAVE ALL",command=savebuttonpressed)
    savebutton.place(x=850,y=40,width=100,height=40)
    exitbutton=Button(root1,text="EXIT",command=exitbuttonpressed)
    exitbutton.place(x=850,y=100,width=100,height=40)

    #code ends for two buttton

    """main code for pump window"""
    #HEADING code
    pump = LabelFrame(root1, text="PUMPS & FLOW RATE", background='gray')
    pump.place(x=10, y=10,width=400,height=200)
    pumplabel=Label(root1,text="PUMPS",background='#ffdbff',borderwidth=1,relief=GROOVE,underline=0)
    pumplabel.place(x=15,y=30,height=20,width=50)
    pumpstatuslabel=Label(root1,text="ACTIVATE",background='#ffdbff',borderwidth=1,relief=GROOVE)
    pumpstatuslabel.place(x=90,y=30,height=20,width=90)
    pumpflowlabel=Label(root1,text="WATER PUMPING RATE(in L/Hr)",background='#ffdbff',borderwidth=1,relief=GROOVE)
    pumpflowlabel.place(x=200,y=30,height=20,width=200)

    #PUMP NO LABELS,ACTIVATEPUMP AND FLOW RATE RATE CODE RESPECTIVELY
    Label(root1,text='PUMP1',borderwidth=1,relief="raised", background='gray').place(x=15,y=52,width=50,height=20)
    Label(root1,text='PUMP2',borderwidth=1,relief="raised", background='gray').place(x=15,y=74,width=50,height=20)
    Label(root1,text='PUMP3',borderwidth=1,relief="raised", background='gray').place(x=15,y=96,width=50,height=20)
    Label(root1,text='PUMP4',borderwidth=1,relief="raised", background='gray').place(x=15,y=118,width=50,height=20)
    Label(root1,text='PUMP5',borderwidth=1,relief="raised", background='gray',state=DISABLED).place(x=15,y=140,width=50,height=20)
    Label(root1,text='PUMP6',borderwidth=1,relief="raised", background='gray',state=DISABLED).place(x=15,y=162,width=50,height=20)
    checkpump1=IntVar()
    checkpump2=IntVar()
    checkpump3=IntVar()
    checkpump4=IntVar()
    checkpump5=IntVar()
    checkpump6=IntVar()
    Checkbutton(root1, text="ACTIVATE", variable=checkpump1,relief=GROOVE, bd=1).place(x=90,y=52,width=90,height=20)
    Checkbutton(root1, text="ACTIVATE", variable=checkpump2,relief=GROOVE, bd=1).place(x=90,y=74,width=90,height=20)
    Checkbutton(root1, text="ACTIVATE", variable=checkpump3,relief=GROOVE, bd=1).place(x=90,y=96,width=90,height=20)
    Checkbutton(root1, text="ACTIVATE", variable=checkpump4,relief=GROOVE, bd=1).place(x=90,y=118,width=90,height=20)
    Checkbutton(root1, text="ACTIVATE", variable=checkpump5,relief=GROOVE, bd=1,state=DISABLED).place(x=90,y=140,width=90,height=20)
    Checkbutton(root1, text="ACTIVATE", variable=checkpump6,relief=GROOVE, bd=1,state=DISABLED).place(x=90,y=162,width=90,height=20)
    pump1rate = IntVar()
    pump2rate = IntVar()
    pump3rate = IntVar()
    pump4rate = IntVar()
    pump5rate = IntVar()
    pump6rate = IntVar()
    Entry(root1, textvariable=pump1rate,width =10).place(x=220,y=52,width=150,height=20)
    Entry(root1, textvariable=pump2rate,width =10).place(x=220,y=74,width=150,height=20)
    Entry(root1, textvariable=pump3rate,width =10).place(x=220,y=96,width=150,height=20)
    Entry(root1, textvariable=pump4rate,width =10).place(x=220,y=118,width=150,height=20)
    Entry(root1, textvariable=pump5rate,width =10,state=DISABLED).place(x=220,y=140,width=150,height=20)
    Entry(root1, textvariable=pump6rate,width =10,state=DISABLED).place(x=220,y=162,width=150,height=20)
    venturi = LabelFrame(root1, text="VENTURI & SUCTION RATE",background='gray')
    venturi.place(x=430, y=10,width=400,height=200)
    venturilabel=Label(root1,text="VENTURI'S",background='#ffdbff',borderwidth=1,relief=GROOVE,underline=0)
    venturilabel.place(x=440,y=30,height=20,width=100)
    venturistatuslabel=Label(root1,text="ACTIVATE",background='#ffdbff',borderwidth=1,relief=GROOVE)
    venturistatuslabel.place(x=550,y=30,height=20,width=70)
    venturiflowlabel=Label(root1,text="SUCTION RATE (in L/Hr)",background='#ffdbff',borderwidth=1,relief=GROOVE)
    venturiflowlabel.place(x=640,y=30,height=20,width=180)

    #PUMP NO LABELS,ACTIVATEPUMP AND FLOW RATE RATE CODE RESPECTIVELY    #font=("Helvetica",9)
    Label(root1,text='VENTURI 1',fg="BLUE",borderwidth=1,relief="raised", background='gray').place(x=440,y=52,width=100,height=15)
    Label(root1,text='VENTURI 2',fg="BLUE",borderwidth=1,relief="raised", background='gray').place(x=440,y=68,width=100,height=15)
    Label(root1,text='VENTURI 3',fg="BLUE",borderwidth=1,relief="raised", background='gray').place(x=440,y=84,width=100,height=15)
    Label(root1,text='VENTURI 4',fg="BLUE",borderwidth=1,relief="raised", background='gray').place(x=440,y=100,width=100,height=15)
    Label(root1,text='VENTURI 5',fg="white",borderwidth=1,relief="raised", background='gray',state=DISABLED).place(x=440,y=116,width=100,height=15)
    Label(root1,text='VENTURI 6',fg="white",borderwidth=1,relief="raised", background='gray',state=DISABLED).place(x=440,y=132,width=100,height=15)
    Label(root1,text='VENTURI 7',fg="white",borderwidth=1,relief="raised", background='gray',state=DISABLED).place(x=440,y=148,width=100,height=15)
    Label(root1,text='VENTURI 8',fg="white",borderwidth=1,relief="raised", background='gray',state=DISABLED).place(x=440,y=164,width=100,height=15)
    Label(root1,text='VENTURI 9',fg="white",borderwidth=1,relief="raised", background='gray',state=DISABLED).place(x=440,y=180,width=100,height=15)


    checkventuri1=IntVar()
    checkventuri2=IntVar()
    checkventuri3=IntVar()
    checkventuri4=IntVar()
    checkventuri5=IntVar()
    checkventuri6=IntVar()
    checkventuri7=IntVar()
    checkventuri8=IntVar()
