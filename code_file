def checkprogram():
    
    def totaltime(x1):
        def getSec1(s):
            l = s.split(':')
            return int(l[0]) * 3600 + int(l[1]) * 60 
        def getSec(s):
            l = s.split(':')
            return int(l[0]) * 3600 + int(l[1]) * 60 + int(l[2])
        def cyclevalues(totalsec,vsec):
            venturion=vsec                         #total venturi on cycle in sec
            venturioff=totalsec-vsec                #total venturi on cycle in sec
            venturioncycle=math.floor(venturion/2)  # to get cycle dive by sec in 2 sec 
            venturioffcycle=math.ceil(venturioff/2) # to get cycle dive by sec in 2 sec 
            if (vsec!=totalsec):
                if (venturioncycle>=venturioffcycle):
                    no_cycles=(math.floor(venturioncycle/venturioffcycle))
                    cycletype='oncycles and equal'
                elif (venturioncycle<venturioffcycle):
                    no_cycles=(math.ceil(venturioffcycle/venturioncycle))
                    cycletype='offcycles'
            else :
                no_cycles=0                    
                cycletype='complete on'
                  
            return (no_cycles,cycletype)  
            
            
            #main code for totaltime starts here
        cstarttime=datetime.strptime(x1[1],"%H:%M")+timedelta(seconds=getSec1(x1[3]))
        cstarttime=str(datetime.time(cstarttime))
        cstoptime=datetime.strptime(x1[2],"%H:%M")-datetime.strptime(x1[4],"%H:%M")
        cstoptime=str(cstoptime)
        k1=cstarttime[0:8]
        k2=cstoptime[0:8]
        dt=datetime.strptime(k2,"%H:%M:%S")-datetime.strptime(k1,"%H:%M:%S")
        dt=str(dt)
        totalsec=getSec(dt)
        vd1sec=getSec1(x1[5])
        vd2sec=getSec1(x1[6])
        vd3sec=getSec1(x1[7])
        (vd1nocycles,vd1cycletype)=cyclevalues(totalsec,vd1sec)
        (vd2nocycles,vd2cycletype)=cyclevalues(totalsec,vd2sec)
        (vd3nocycles,vd3cycletype)=cyclevalues(totalsec,vd3sec)
        return (dt,vd1nocycles,vd1cycletype,vd2nocycles,vd2cycletype,vd3nocycles,vd3cycletype)
    def slot1fun():
    
        f=open('/home/yatish/Desktop/spe_workspace/softwarefiles/temporary.txt','rb')
