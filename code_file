def loadprogram():
   print(" code for load complete program including all slots")

   myfiletypes = [('Program files', '*.txt')]
   filename=askopenfilename(filetypes = myfiletypes,initialdir='/home/yatish/Desktop/spe_workspace/programs',title='Load the Program')
   f=open(filename,'rb')
   x1=pickle.load(f)
   print(x1)
   (slot1_name.set(x1[0]),slot1main_start.set(x1[1]),slot1main_stop.set(x1[2]),slot1_pre.set(x1[3]),slot1_post.set(x1[4]),slot1_vd1.set(x1[5]),slot1_vd2.set(x1[6]),slot1_vd3.set(x1[7]),slot1_vd4.set(x1[8]),
            slot1_vd5.set(x1[9]),slot1_vd6.set(x1[10]),slot1_vd7.set(x1[11]),slot1_vd8.set(x1[12]),
            slot1_vd9.set(x1[13]),slot1_priorityno.set(x1[14]),vslot1_1.set(x1[15]),vslot1_2.set(x1[16]),
            vslot1_3.set(x1[17]),vslot1_4.set(x1[18]),vslot1_5.set(x1[19]),vslot1_6.set(x1[20]),vslot1_7.set(x1[21]),
            vslot1_8.set(x1[22]),vslot1_9.set(x1[23]),vslot1_10.set(x1[24]),vslot1_11.set(x1[25]),vslot1_12.set(x1[26]),
            vslot1_13.set(x1[27]),vslot1_14.set(x1[28]),vslot1_15.set(x1[29]),vslot1_16.set(x1[30]),vslot1_17.set(x1[31]),
            vslot1_18.set(x1[32]),vslot1_19.set(x1[33]),vslot1_20.set(x1[34]),vslot1_21.set(x1[35]),vslot1_22.set(x1[36]),vslot1_23.set(x1[37]),
            vslot1_24.set(x1[38]),vslot1_25.set(x1[39]),vslot1_26.set(x1[40]),vslot1_27.set(x1[41]),vslot1_28.set(x1[42]),vslot1_29.set(x1[43]),vslot1_30.set(x1[44]))

   x2=pickle.load(f)
   print(x2)
   (slot2_name.set(x2[0]),slot2main_start.set(x2[1]),slot2main_stop.set(x2[2]),slot2_pre.set(x2[3]),slot2_post.set(x2[4]),slot2_vd1.set(x2[5]),slot2_vd2.set(x2[6]),slot2_vd3.set(x2[7]),slot2_vd4.set(x2[8]),
            slot2_vd5.set(x2[9]),slot2_vd6.set(x2[10]),slot2_vd7.set(x2[11]),slot2_vd8.set(x2[12]),
            slot2_vd9.set(x2[13]),slot2_priorityno.set(x2[14]),vslot2_1.set(x2[15]),vslot2_2.set(x2[16]),
